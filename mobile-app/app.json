{
  "expo": {
    "name": "Maternal & Child Health",
    "slug": "maternal-child-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "maternal-child-app",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.maternalchildapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yourcompany.maternalchildapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "c286e119-34e3-4801-9c14-38da99930e1f"
      }
    },
    "owner": "darkgrechers-organization",
    "plugins": [
      "expo-web-browser",
      [
        "react-native-auth0",
        {
          "domain": "dev-e5znz5x47bkxbit3.us.auth0.com"
        }
      ],
      "expo-video"
    ]
  }
}
